@model List<HotelProject.UI.Dtos.DashboardDtos.LastStaffDto>

<div class="row">
    @foreach(var item in Model)
    {
        <div class="col-lg-3 col-sm-6">
        <div class="card">
            <div class="card-body">
                <div class="text-center">
                        <img src="@item.SocialMedia1" class="rounded-circle" alt="" style="height:75px; width:75px;">
                    <h5 class="mt-3 mb-1">@item.Name</h5>
                    <p class="m-0">@item.Title</p>
                    
                </div>
            </div>
        </div>
    </div>
    }
    
</div>